{
    "name": "olaurendeau/rabbit-mq-admin-toolkit-bundle",
    "description": "Automate rabbitmq vhost's configuration creation / update",
    "type": "symfony-bundle",
    "require": {
        "symfony/config": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "symfony/http-kernel": "~2.1",
        "richardfullmer/rabbitmq-management-api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Laurendeau",
            "email": "o.laurendeau@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Ola\\RabbitMqAdminToolkitBundle": "" }
    },
    "target-dir": "Ola/RabbitMqAdminToolkitBundle",
    "config": {
        "bin-dir": "bin"
    }
}
